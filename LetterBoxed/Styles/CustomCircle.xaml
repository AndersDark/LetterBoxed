<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="CustomCircle" TargetType="Ellipse">
        <Setter Property="Width" Value="11"/>
        <Setter Property="Height" Value="11"/>
        <Setter Property="Canvas.ZIndex" Value="5"/>
        <Setter Property="Fill" Value="{StaticResource BoxCircleUnusedColor}"/>
        <Setter Property="Stroke" Value="{StaticResource BoxCircleBorderColor}"/>
        <Setter Property="StrokeThickness" Value="1.5"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>
</ResourceDictionary>